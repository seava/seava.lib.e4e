<project name="DistributionPackageBuilder.extract" default="extract" basedir="." xmlns:ac="antlib:net.sf.antcontrib">
	<description>Extract jar content.</description>
	<property file="pack.properties" />


	<target name="extract">
		<ac:for list="${coreBundles}" param="bundle">
			<sequential>
				<unzip src="${tempFolder}/no-version-bundles/seava.e4e.@{bundle}.jar" dest="${tempFolder}/no-version-bundles/seava.e4e.@{bundle}">
					<patternset>
						<exclude name="META-INF/**" />
					</patternset>
				</unzip>
			</sequential>
		</ac:for>
	</target>
</project>